{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": ["dom", "esnext"]
  },
  "include": ["./src/**/*", "./src/gatsby-types.d.ts"],
  "exclude": ["node_modules", ".cache", "public"]
}